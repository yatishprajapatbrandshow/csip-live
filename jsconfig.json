{
  "compilerOptions": {
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@Components/*": [
        "./src/Components/*"
      ],
      "@utils/*": [
        "./src/utils/*"
      ],
      "@redux/*": [
        "./src/redux/*"
      ],
      "@styles/*": [
        "./src/styles/*"
      ],
      "@fonts/*": [
        "./src/fonts/*"
      ],
      "@image/*": [
        "./src/images/*"
      ]
    }
  }
}